@page "/das"

<MudContainer MaxWidth="MaxWidth.ExtraLarge" Class="mt-10">
    <MudCard Style="height:100%;">
        <MudCardHeader>
            <CardHeaderContent>
                <MudText Typo="Typo.h6">Earning Report</MudText>
            </CardHeaderContent>
        </MudCardHeader>
        <MudCardContent Class="px-0">
            <MudSimpleTable Elevation="0" Hover="true">
                
            </MudSimpleTable>
        </MudCardContent>
    </MudCard>
</MudContainer>

<MudDialog>
    <TitleContent>
        <MudText Typo="Typo.h6">
            <MudIcon Icon="@Icons.Material.Filled.DeleteForever" Class="mr-3 mb-n1" />
            Delete server?
        </MudText>
    </TitleContent>
    <DialogContent>
        <MudTextField Value="1" Label="Server ID" ReadOnly="true" />
    </DialogContent>
    <DialogActions>
        <MudButton >Cancel</MudButton>
        <MudButton Color="Color.Error" >Delete Server</MudButton>
    </DialogActions>
</MudDialog>

